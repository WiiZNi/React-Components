{
  "name": "tactileo.designsystem",
  "version": "0.1.5",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://pkgs.dev.azure.com/maskott/Tactileo.DesignSystem/_packaging/tactileo.designsystem/npm/registry/"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-collapsed": "^3.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "optimize-svg": "svgo -f ./src/assets/icons -r",
    "convert-svg": "svgr -d ./src/stories/Foundation/Icons/assets ./src/assets/icons --replace-attr-values #111={props.color} --ignore-existing"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-controls": "^6.3.4",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "storybook-addon-designs": "^6.0.0",
    "style-loader": "^2.0.0"
  }
}
